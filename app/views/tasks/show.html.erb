<h1>Task</h1>

    <h3><%= @task.details%></h3>
    <br>
    <h3><%= @task.completed %></h3>
    <br>
<h2><%=link_to "Back to", tasks_path%></h2>
<h2><%=link_to "Edit task", edit_task_path(@task.id)%></h2>
<%= link_to "Delete",
  task_path(@task),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
%>
